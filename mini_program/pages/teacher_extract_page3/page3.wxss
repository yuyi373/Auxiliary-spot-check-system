.Section{
  display: flex;
  justify-content: center; 
  align-items: center; 
  flex-direction: column;
  font-size: 12px;
  width: 100%;
  height: 220rpx;
  line-height: 60rpx;
  /* border-bottom: solid 1px darkgray; */
  margin-bottom: 25rpx;

}
.line{
  margin-bottom: 30rpx;
  width: 88%;
  height: 0;
  border-bottom: 1.5px solid #d5dee4;
}

.turn,.Start,.Later{
    height: 50%;
    display: flex;
    align-items: center;
    justify-content: center;  
    font-size: 32rpx;
    color: #4C4C4F;
}

.turn{
  color:#7AB8FB;
  display: flex;
  align-items: center;
  justify-content: center;  
}

image{
  width:55rpx;
  height:55rpx;
}


.text{
  margin-left: 20rpx;
}

.table {
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 28rpx 0;
  width: 100%;
  font-size: 15px;
  flex-direction: column;
}

.tr {
  display: flex;
  width: 90%;
  height: 2rem;
  border: 1rpx solid #eff4f8;
  background: #b2dffe;
  /* justify-content: space-between; */
}

.td,.th {
  display: flex;
  justify-content: center;  /*水平居中 */
  align-items: center; /*垂直居中*/
  width: 100%;
  /*border: 1px solid darkgray;*/
  height: 2rem;
  border: 1px solid #eff4f8;
}

.view-wrap{
  border: solid 1px;
  display: flex; 
  flex-direction: row;
}
